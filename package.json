{
    "homepage": "./",
    "name": "pinata-cloudflare-demo",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@types/node": "^17.0.4",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "nth-check": "^2.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "^4.0.3",
        "ts-node": "^10.4.0",
        "typescript": "4.4.4",
        "web-vitals": "^2.1.2",
        "yarn": "^1.22.17"
    },
    "scripts": {
        "start": "react-scripts --openssl-legacy-provider start",
        "build": "react-scripts --openssl-legacy-provider build",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watchAll",
        "test:snapshot": "jest --updateSnapshot",
        "eject": "react-scripts eject",
        "lint": "eslint \"**/*.{ts,tsx}\"",
        "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
        "clean:powershell": "powershell Remove-Item './node_modules', './package-lock.json', './yarn.lock', './build' -Force -Recurse -ea 0",
        "clean:bash": "rm ./node_modules -fr && rm ./package-lock.json -fr && rm ./yarn.lock -fr && rm ./build -fr"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@jest/globals": "^27.3.1",
        "@testing-library/dom": "^8.5.0",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.0.3",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "babel-jest": "^26.6.0",
        "eslint": "^7.32.0",
        "eslint-config-next": "latest",
        "eslint-config-standard": "^16.0.3",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-react": "^7.25.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-sonarjs": "^0.10.0",
        "eslint-plugin-testing-library": "^4.12.4",
        "jest": "26.6.0",
        "msw": "^0.35.0",
        "react-test-renderer": "^17.0.2"
    }
}